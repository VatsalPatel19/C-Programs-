#include<stdio.h>
#include<conio.h>
int main()
{
 int no;
 printf("Enter a number between 1 to 9:");
 scanf("%d",&no);
 printf("\n The Number in words is:");
switch(no)
{
 case 1:
 printf("\nONE");
 break;
 case 2:
 printf("\nTWO");
 break;
 case 3:
 printf("\nTHREE");
 break;
 case 4:
 printf("\nFOUR");
 break;
 case 5:
 printf("\nFIVE");
 break;
 case 6:
 printf("\nSIX");
 break;
 case 7:
 printf("\nSEVEN");
 break;
 case 8:
 printf("\nEIGHT");
 break;
 case 9:
 printf("\nNINE");
 break;
}
}
